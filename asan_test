#!/bin/bash
rm -rf asan_result.log 
for i in {1..26}
do
    echo "Executing Test $i"
    echo "=================================================================================================================================" >>asan_result.log 2>&1
    echo "== Starting Test $i" >>asan_result.log 2>&1
    echo "=================================================================================================================================" >>asan_result.log 2>&1
    MCTester_ASAN $i $i >>asan_result.log 2>&1
    echo "=================================================================================================================================" >>asan_result.log 2>&1
done
